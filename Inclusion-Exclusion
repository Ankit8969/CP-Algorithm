#include <bits/stdc++.h>
using namespace std;

bool isValid(char ch) {
    return (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u');
}

void code() {
    int n;
    cin >> n;
    vector<string> v_string;
    for(int i=0;i<n;i++) {
        string str;
        cin >> str;
        v_string.push_back(str);
    }
    map<string,long long> subset_hash;
    for (int i=0;i<n;i++) {
        // Extrating the vowel
        set<char> vowel;
        for (auto x: v_string[i]) {
            if (isValid(x))
                vowel.insert(x);
        }
        // insert into the string, easy to iterate / modify
        string str = "";
        for (auto x: vowel)
            str += x;

        for (int mask = 0;mask < (1 << str.length());mask++) {
            string subset = "";
            for (int i=0;i<str.length();i++){
                if ((mask & (1 << i)) != 0){
                    subset += str[i];
                }
            }
            if (subset.length() > 0) {
                subset_hash[subset]++;
            }
        }
    }
    long long ans = 0;
    for (auto &x: subset_hash) {
        if (x.second < 3)
            continue;
        long long nCr = (long long)((x.second) * (x.second - 1) * (x.second - 2))/6;
        if (x.first.length() % 2)
            ans += nCr;
        else
            ans -= nCr;
    }
    cout << ans << endl;
}

int main() {
	int test;
	cin >> test;
	while(test--) {
	    code();
	}
	return 0;
}






Input

2
5
helen
irena
arthos
pentos
tulu
3
alen
bira
cult







